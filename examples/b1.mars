 fn factorial(n: i64) -> i64 {
    if n <= 1 {
        return 1;
    }

    var prev: i64 = factorial(n - 1);
    return n * prev;
}


fn main() -> i64 {
    var start = now();

    var x: i64 = factorial(20);
    print_i64(x);
    println("");

    var timeSpend = now() - start;
    print_i64(timeSpend);
    println("");

    return 0;
}